_ios_run-tests() {
    local -i ret=1
    local -ar arg_specs=(
        '*:globs:'
        '--release-mode[Generate a release project for testing (unable to utilize Tuist'\''s binary cache)]'
        '--excluding[A list of regexes to use to exclude modules.]:excluding:'
        '--workspace[The name to use for the generated workspace, if generating with Tuist binaries.]:workspace:'
        '(--output -o)'{--output,-o}'[The directory in which to store xcresult and log output.]:output:'
        '--repetitions[An option to specify a number of test repetitions. Defaults to no repetitions.]:repetitions:'
        '--relaunch-on-repetition[A flag to specify to relaunch tests on repetition. Defaults to false.]'
        '--only-testing[An option to pass a value for -only-testing to xcodebuild. This option is only valid if running on a single module.
        As of Xcode 16.1, this format of this value is `TestTarget\[/TestClass\[/TestMethod\]\]`, e.g. `UserServiceTests/UserServiceConsumerTests/testUserServiceCanLogout`.
        Check out `man xcodebuild` for the most up-to-date documentation on this option.]:only-testing:'
        '--block-mr-on-failure[Whether failure should manually block the current MR (based on CI environment variables).]'
        '--upload-to-artifactory[Whether the results of running the command should be uploaded to artifactory (CI only)]'
        '--quarantine-failures[Whether test failures should be quarantined using Trunk (utilized CI environment variables).]'
        '--skip-generation[If passed, all project generation steps will be skipped.]'
        '--skip-build[If passed, all test scheme build steps will be skipped.]'
        '--skip-derived-data-clean[If passed, derived data cleanup will be skipped.]'
        '--randomized-order[Randomizes the order of module testing]'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '--swift6-mode[Add all the upcoming Swift 6 features. Example\: `--swift6-mode`]'
        '--swift6-modules[Set a list of modules that are known to support swift 6. Example\: `--swift6-modules=ModuleA ModuleB`.]:swift6-modules:'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_auth() {
    local -i ret=1
    local -ar arg_specs=(
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'login:Logs in the local workstation with Tuist Cloud server'
            'logout:Logs out the user'\''s local workstation from Tuist Cloud server'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        login|logout)
            "_ios_auth_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"
}

_ios_auth_login() {
    local -i ret=1
    local -ar arg_specs=(
        '--prod[The Tuist Cloud environment one wishes to work against.]'
        '--dev[The Tuist Cloud environment one wishes to work against.]'
        '--tuist-cloud[The Tuist Cloud environment one wishes to work against.]'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}


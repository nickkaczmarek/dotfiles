#compdef ios

__ios_complete() {
    local -ar non_empty_completions=("${@:#(|:*)}")
    local -ar empty_completions=("${(M)@:#(|:*)}")
    _describe -V '' non_empty_completions -- empty_completions -P $'\'\''
}

__ios_custom_complete() {
    local -a completions
    completions=("${(@f)"$("${command_name}" "${@}" "${command_line[@]}")"}")
    if [[ "${#completions[@]}" -gt 1 ]]; then
        __ios_complete "${completions[@]:0:-1}"
    fi
}

__ios_cursor_index_in_current_word() {
    if [[ -z "${QIPREFIX}${IPREFIX}${PREFIX}" ]]; then
        printf 0
    else
        printf %s "${#${(z)LBUFFER}[-1]}"
    fi
}

_ios() {
    emulate -RL zsh -G
    setopt extendedglob nullglob numericglobsort
    unsetopt aliases banghist

    local -xr SAP_SHELL=zsh
    local -x SAP_SHELL_VERSION
    SAP_SHELL_VERSION="$(builtin emulate zsh -c 'printf %s "${ZSH_VERSION}"')"
    local -r SAP_SHELL_VERSION

    local context state state_descr line
    local -A opt_args

    local -r command_name="${words[1]}"
    local -ar command_line=("${words[@]}")
    local -ir current_word_index="$((CURRENT - 1))"

    local -i ret=1
    local -ar arg_specs=(
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'create-module:Walk a user through defining a new module'
            'generate:Generates a project/workspace of a module'
            'generate-docs:Generates documentation for modules'
            'sync:Syncs and silently re-generates all the modules in the repo'
            'version:Returns the version of the installed ios-cli tool'
            'edit:Edits manifest files'
            'clean:Modules cleaning tool'
            'upgrade:Upgrades the ios-cli'
            'score:Evaluates and scores the quality of a given module'
            'score-config:Commands for module score configuration'
            'debug:Edits manifest files'
            'fix:Tries to run some automated fixes for working in the monorepo'
            'graph:Creates a JSON representation of the graph of module dependencies'
            'compliance:Umbrella command that encompasses a collection of sub-commands related to compliance'
            'gitlab:This is the subcommand that namespaces things related to gitlab CI generation'
            'check:This is the subcommand that namespaces checks and verification jobs'
            'run-tests:Generates, builds, and tests module unit tests.'
            'auth:Authenticates the local workstation with Tuist Cloud server'
            'upload-activitylogs:A tool to send xcactivitylog files for a specific build to S3 via a custom service'
            'codeowner:'
            'install:Install external dependencies for project generation.'
            'module:'
            'flaky-test:'
            'artifactory:A CLI to perform actions with artifactory.'
            'json-schema:This is the subcommand that namespaces json-schema jobs'
            'repo:A group of commands related to repo management'
            'help:Show subcommand help information.'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        create-module|generate|generate-docs|sync|version|edit|clean|upgrade|score|score-config|debug|fix|graph|compliance|gitlab|check|run-tests|auth|upload-activitylogs|codeowner|install|module|flaky-test|artifactory|json-schema|repo|help)
            "_ios_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"
}

_ios_create-module() {
    local -i ret=1
    local -ar ___type=('generic' 'zgGraph')
    local -ar ___extra_targets=('library' 'unittests' 'mocks' 'graphQLCodegen' 'swiftUIApplication' 'uikitApplication')
    local -ar arg_specs=(
        '--name[The module'\''s name; if not specified will ask for interactive input.]:name:'
        '--team[The team name owning this module; if not specified will ask for interactive input.]:team:'
        '--type[The module type to generate; if not specified will ask for interactive input or default to generic.]:type:{__ios_complete "${___type[@]}"}'
        '*'{--extra-targets,-e}'[Extra targets to add, optional.]:extra-targets:{__ios_complete "${___extra_targets[@]}"}'
        '--apple-id[The Apple ID to use to sign example apps (if present); if not specified will ask for interative input or default to empty.]:apple-id:'
        '--resources[Indicates if the module will host resources, default is false, in interactive mode will ask input from the user]:resources:'
        '--docc[Indicates if the module should include Documentation.docc folders in the initial target, default is false, in interactive mode will ask input from the user]:docc:'
        '--swift6-mode[Add all the upcoming Swift 6 features]'
        '--skip-generation[Skip project generation after module creation]'
        '--enable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '--disable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_generate() {
    local -i ret=1
    local -ar arg_specs=(
        '*:globs:_path_files -g "*"'
        '--app'
        '--module'
        '--automatic'
        '(--retry -r)'{--retry,-r}'[Optional retries to indicate the command should retry if failed.]:retry:'
        '(--binary-cache -b)'{--binary-cache,-b}'[Will cause Tuist to use a binary cache or not, not relevant for --release-mode, default to true]'
        '--no-binary-cache[Will cause Tuist to use a binary cache or not, not relevant for --release-mode, default to true]'
        '--release-mode[Will cause the generate command to produce a release like project, default to false, note does not work with the binary cache option]'
        '--prod[The Tuist Cloud environment one wishes to work against.]'
        '--dev[The Tuist Cloud environment one wishes to work against.]'
        '--tuist-cloud[The Tuist Cloud environment one wishes to work against.]'
        '--enable-liquid-glass-ui[Disables/Enables Liquid Glass UI for Application targets (Debug builds only, release builds default to OFF)]'
        '--disable-liquid-glass-ui[Disables/Enables Liquid Glass UI for Application targets (Debug builds only, release builds default to OFF)]'
        '--sparse[EXPERIMENTAL feature to enable sparse checkout of generated modules]'
        '--enable-skip-quarantined-tests[Disable/Enable local skipping of quarantined tests]'
        '--disable-skip-quarantined-tests[Disable/Enable local skipping of quarantined tests]'
        '--enable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '--disable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '--swift6-mode[Add all the upcoming Swift 6 features. Example\: `--swift6-mode`]'
        '--swift6-modules[Set a list of modules that are known to support swift 6. Example\: `--swift6-modules=ModuleA ModuleB`.]:swift6-modules:'
        '--no-open[Tuist generation flags, to allow fine grain control over the underlying `tuist generate` command]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}


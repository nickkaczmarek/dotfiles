_ios_gitlab_label-tickets() {
    local -i ret=1
    local -ar arg_specs=(
        '--config-path[The path to the config file, relative to repo root.]:config-path:'
        '(--release-config-key -r)'{--release-config-key,-r}'[The key of the appConfig within the provided config file to work with]:release-config-key:'
        '--version[The version to tag the tickets with]:version:'
        '(--commits -c)'{--commits,-c}'[A fallback number of commits to fetch from Gitlab to search tickets for in case of non-existing tag. This is mainly to support an initial release where a tag doesn'\''t exist yet.]:commits:'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_gitlab_generate-rollback-pipeline() {
    local -i ret=1
    local -ar ___source=('mergeTrain' 'mergeRequest' 'branch')
    local -ar ___type=('standard' 'scheduled')
    local -ar arg_specs=(
        '--release-config-path[The path to the release config file, relative to repo root.]:release-config-path:'
        '(--release-config-key -r)'{--release-config-key,-r}'[The key of the appConfig within the provided config file to work with]:release-config-key:'
        '(--source -s)'{--source,-s}'[The source of the pipeline (options\: mergeRequest, mergeTrain, main). If no value is supplied, the CLI will attempt to infer it using environment values]:source:{__ios_complete "${___source[@]}"}'
        '(--type -t)'{--type,-t}'[The type of the pipeline (options\: standard, scheduled). If no value is supplied, the CLI will attempt to infer it using environment values]:type:{__ios_complete "${___type[@]}"}'
        '(--branch -b)'{--branch,-b}'[The name of the branch we are generating CI for. If no value is supplied, the CLI will attempt to infer it using environment values]:branch:'
        '(--output-prefix -o)'{--output-prefix,-o}'[Output filename prefix]:output-prefix:'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_gitlab_update-scheduled-pipelines() {
    local -i ret=1
    local -ar arg_specs=(
        '--config-folder[The folder in which to look for schedule configs]:config-folder:'
        '--gitlab-project-id[The gitlab project ID for the project in which to create the scheduled pipelines. Attempts to read from CI_PROJECT_ID if not provided.]:gitlab-project-id:'
        '--gitlab-api-url[The gitlab API URL to use when creating the scheduled pipelines. Attempts to read from CI_API_V4_URL if not provided.]:gitlab-api-url:'
        '--gitlab-api-token[The gitlab API token to use when creating the scheduled pipelines. Attempts to read from GITLAB_API_TOKEN if not provided.]:gitlab-api-token:'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_check() {
    local -i ret=1
    local -ar arg_specs=(
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'ci-yaml-extends:Verifies that all ci.yml files in the repo only extend from valid script/when templates as defined in pipeline-templates.yml'
            'file-names:Verifies that all files in the repo have a valid file name'
            'always-inflated-ci-yamls:A tool to verify that the always inflated ci ymls file is up to date'
            'lock-files:Verifies that all of our lock files are up to date'
            'implicit-imports:Verifies implicit imports for an already generated module.
Please make sure to run `ios generate...` with the desired module(s) before running this command.

No need to supply with underlying modules, run `ios check implicit-imports` after generation, and that is it.'
            'subrepo-dependencies:Verifies that subrepos have valid dependencies.'
            'no-empty-test-runs:Verifies that a given xcresult file, test count is not zero. This allows us to make sure test run is not empty (i.e. that no tests were run)'
            'no-empty-tests:Verifies that a module does not have any empty test targets.'
            'prebuilt-cli-hash:A tool to verify that a prebuilt CLI we have committed to the repo is up to date'
            'no-conflict-markers:A tool for checking whether any files in a folder have conflict markers.'
            'no-git-changes:A tool to make sure that there are no git changes in specific files'
            'ios-deployment-version:A tool for verifying the deployment version for all targets in an xcodeproj.'
            'cursor-rules:Verifies that Cursor rules do not contain any hidden malicious characters potentially usable to inject poisoned code.'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        ci-yaml-extends|file-names|always-inflated-ci-yamls|lock-files|implicit-imports|subrepo-dependencies|no-empty-test-runs|no-empty-tests|prebuilt-cli-hash|no-conflict-markers|no-git-changes|ios-deployment-version|cursor-rules)
            "_ios_check_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"
}

_ios_check_ci-yaml-extends() {
    local -i ret=1
    local -ar arg_specs=(
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}


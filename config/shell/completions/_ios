_ios_repo() {
    local -i ret=1
    local -ar arg_specs=(
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'sparse-checkout:This command is used to create sparse checkouts, which change the working tree from having all tracked files present to only having a subset files based on dependency analysis.'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        sparse-checkout)
            "_ios_repo_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"
}

_ios_repo_sparse-checkout() {
    local -i ret=1
    local -ar arg_specs=(
        '*:project:'
        '(--max-concurrency -m)'{--max-concurrency,-m}'[Maximum number of concurrent graph resolutions.]:max-concurrency:'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_help() {
    local -i ret=1
    local -ar arg_specs=(
        '*:subcommands:'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios

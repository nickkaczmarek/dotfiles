_ios_clean() {
    local -i ret=1
    local -ar arg_specs=(
        '(--path -p)'{--path,-p}'[Optional path to recursively iterate and clean]:path:'
        '-g[Run deep git clan `git clean -ffxd`.]'
        '-d[Deletes derived data, in combination with --fast/-f will just send it to the Trash.]'
        '-s[Deletes SPM artifacts, in combination with --fast/-f will just send it to the Trash.]'
        '-c[Cleans Tuist cache related artifacts binaries, dependencies.]'
        '-x[Kills all xcode related process in the system.]'
        '--fast[Making folder deletion fast!]'
        '--stash[Automatically stash git changes, mainly relevant to the --git-clean/-g flag.]'
        '*--git-clean-excludes[Paths to excluding when running git clean. Only relevant when -g is used. Defaults to \[".cursor", ".cursorrules", ".claude", "CLAUDE.md", "CLAUDE.local.md", "AGENT.md", "AGENTS.md", "GEMINI.md", "crush.json", ".crush.json", ".crush", ".rules", ".windsurfrules", ".clinerules", ".vscode", ".zed", "buildserver.json"\]]:git-clean-excludes:'
        '--enable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '--disable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_upgrade() {
    local -i ret=1
    local -ar ___formulas=('ios' 'muter' 'tuist')
    local -ar arg_specs=(
        '--update[Indicates whether "brew update" should be run.]'
        '--no-update[Indicates whether "brew update" should be run.]'
        '(--force -f)'{--force,-f}'[Force upgrades, even if no upgrade is detected as needed]'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '*--formulas[The desired formulas to upgrade, default all cases - \[ModuleCLI.EnumerableHomebrewFormula.ios, ModuleCLI.EnumerableHomebrewFormula.muter, ModuleCLI.EnumerableHomebrewFormula.tuist\]]:formulas:{__ios_complete "${___formulas[@]}"}'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_score() {
    local -i ret=1
    local -ar ___metrics=('codeCoverage' 'codeowners' 'dependencyAnalysis' 'publicSymbols' 'mutationTesting' 'deadCodeAnalysis')
    local -ar arg_specs=(
        '--score-config[The path to a config file defining which modules to score]:score-config:'
        '*:paths:'
        '--team[If this option is passed, then all apps and modules that are owned by that team (based on CODEOWNERS) are scored.
        This option is ignored if specific modules or apps are passed via the "paths" argument.]:team:'
        '--static[Run the score using static analysis (this is the fastest, but least accurate score).]'
        '--single-pass[Run the score using a dynamic analysis, but expect every metric in the score to build/test/etc only once (moderate accuracy, but could be a bit of a wait).]'
        '--detailed[Run the most accurate and detailed score, regardless of the time it takes (depending on module complexity this could take many hours, might be the kind of thing you run on Friday and check on Monday).]'
        '--format[Generates the report in the passed format.
        Some formats will print to stdout by default (text, csv), while others will generate files and/or folders by default (html).
        The "output" flag may be used to modify the location of where the report is generated.]:format:'
        '(--output -o)'{--output,-o}'[A path to an optional output location. If specified, will print the report to this file instead of stdout.
        For report types that only output to a file or folder, if this value is not specified, the default ".report" will be used.]:output:'
        '(--input -i)'{--input,-i}'[A path to an input JSON file. If provided, the score does not re-calculate metrics, but rather uses the JSON data to generate a new report.
        This file should have be generated by running the score previously with "--format json".]:input:'
        '(--metrics -m)'{--metrics,-m}'[A list of metric to evaluate for this report.
        If metrics are specified explicitly, then those metrics will be included regardless of the "accuracy" flag passed to the report.]:metrics:{__ios_complete "${___metrics[@]}"}'
        '--skip-generation[An option to skip project generation when running the score in single-pass or detailed mode (project generation is not run in static mode).
        You can add this flag if you'\''ve already generated the project yourself.]'
        '--skip-clean[An option to skip xcodebuild cleaning.]'
        '--skip-build[An option to skip xcodebuild building (implies that cleaning should be skipped).]'
        '--include-apps[When running the score for multiple modules (e.g. for a team), this flag indicates that it should include apps.]'
        '--exclude-apps[When running the score for multiple modules (e.g. for a team), this flag indicates that it should exclude apps.]'
        '--include-fixtures[When running the score for multiple modules (e.g. for a team), this flag indicates that it should include fixtures.]'
        '--exclude-fixtures[When running the score for multiple modules (e.g. for a team), this flag indicates that it should exclude fixtures.]'
        '--excluding[An option to exclude modules with a path that contains any of the passed substring values.]:excluding:'
        '--incremental[Indicates that the score can attempt to utilize file artifacts in the .modulecli/score from previous runs.
        This is an advanced feature; do not use unless you understand how the file artifacts affect score behavior.]'
        '--plan-only[Only print out the module scoring plan, without running.]'
        '--enable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '--disable-auto-upgrade[This flag enables/disables auto-upgrading of the ios-cli.
        The following environment variables are available to adjust auto-upgrade behavior in your environment\:
        - IOS_CLI_AUTO_UPGRADE\: disabled, enabled (default)
        - IOS_CLI_UPGRADE_ACCURACY\: accurate (default), fast
        If passed, this flag will override the behavior of IOS_CLI_AUTO_UPGRADE.]'
        '--force[Force running the report for unsupported paths. This is not a recommended feature.]'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_score-config() {
    local -i ret=1
    local -ar arg_specs=(
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'validate:Validates module score configs'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        validate)
            "_ios_score-config_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"
}


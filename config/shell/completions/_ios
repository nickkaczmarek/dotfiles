        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'diff:Returns a list of codeowners for changed files on current branch'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        diff)
            "_ios_codeowner_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"

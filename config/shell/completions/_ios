_ios_flaky-test() {
    local -i ret=1
    local -ar arg_specs=(
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'download:Downloads the Trunk CLI for future use'
            'upload:Uploads xcresult data to Trunk'
            'quarantine:Quarantines the xcresult test output based on Trunk'\''s determination of tests to suppress'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        download|upload|quarantine)
            "_ios_flaky-test_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"
}


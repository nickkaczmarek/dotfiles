_ios_gitlab() {
    local -i ret=1
    local -ar arg_specs=(
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
        '(-): :->command'
        '(-)*:: :->arg'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0
    case "${state}" in
    command)
        local -ar subcommands=(
            'generate:A tool to generate a CI yml based on changed files, pipeline source and pipeline type'
            'inflate:A tool to inflate all jobs in a gitlab ci yaml that extend a .auto- template'
            'dump-templates:A tool to dump gitlab templates to a file'
            'dump-always-inflated-ci-yamls:A tool to dump ci.yml files that should always be inflated under various conditions to a file'
            'dump-code-coverage:A tool to dump code coverage information to a file, intended for use in reporting coverage for gitlab jobs'
            'cache:A tool to cache binary modules and external libraries in CI, based on changed files, pipeline source and pipeline type'
            'tuist-prepare-for-parallel:Runs a tuist dump command to prepare tuist for running stuff in parallel'
            'label-tickets:A tool used to label JIRA tickets with a certain release tag and version'
            'generate-rollback-pipeline:A tool to generate ci yml definitions for a rollback pipeline'
            'update-scheduled-pipelines:'
        )
        _describe -V subcommand subcommands
        ;;
    arg)
        case "${words[1]}" in
        generate|inflate|dump-templates|dump-always-inflated-ci-yamls|dump-code-coverage|cache|tuist-prepare-for-parallel|label-tickets|generate-rollback-pipeline|update-scheduled-pipelines)
            "_ios_gitlab_${words[1]}"
            ;;
        esac
        ;;
    esac

    return "${ret}"
}

_ios_gitlab_generate() {
    local -i ret=1
    local -ar ___source=('mergeTrain' 'mergeRequest' 'branch')
    local -ar ___type=('standard' 'scheduled')
    local -ar arg_specs=(
        '(--source -s)'{--source,-s}'[The source of the pipeline (options\: mergeRequest, mergeTrain, main). If no value is supplied, the CLI will attempt to infer it using environment values]:source:{__ios_complete "${___source[@]}"}'
        '(--type -t)'{--type,-t}'[The type of the pipeline (options\: standard, scheduled). If no value is supplied, the CLI will attempt to infer it using environment values]:type:{__ios_complete "${___type[@]}"}'
        '(--branch -b)'{--branch,-b}'[The name of the branch we are generating CI for. If no value is supplied, the CLI will attempt to infer it using environment values]:branch:'
        '--inflate-all-yamls[Should we inflate all ci.yml files in the repo. By default, will use dependency analysis to only inflate ci.yml files in the dependency tree]'
        '(--output-prefix -o)'{--output-prefix,-o}'[Output filename prefix]:output-prefix:'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}

_ios_gitlab_inflate() {
    local -i ret=1
    local -ar ___source=('mergeTrain' 'mergeRequest' 'branch')
    local -ar ___type=('standard' 'scheduled')
    local -ar arg_specs=(
        '(--source -s)'{--source,-s}'[The source of the pipeline (options\: mergeRequest, mergeTrain, main). If no value is supplied, will default to mergeRequest]:source:{__ios_complete "${___source[@]}"}'
        '(--type -t)'{--type,-t}'[The type of the pipeline (options\: standard, scheduled). If no value is supplied, will default to standard]:type:{__ios_complete "${___type[@]}"}'
        '(--branch -b)'{--branch,-b}'[The name of the branch we are generating CI for. If no value is supplied, will default to an empty string]:branch:'
        '--include-stages[When passed, stages are included in the output yaml]'
        '--infer-from-environment[When passed, CI settings (source, type, branch) are inferred from the environment, instead of using defaults]'
        '*'{--changed-files,-c}'[A list of changed files to simulate the inflation with. If no value is supplied, will default to the location of the input file]:changed-files:'
        '(--output -o)'{--output,-o}'[Output filename]:output:'
        '--graph[Manually specify whether a dependency graph should be created and/or skipped.
        This command will automatically detects whether a graph is needed, based on whether dependencies should be considered when inflating yamls.
        This flag is useful if you'\''d like to inflate a module'\''s yaml without considering its dependencies.]'
        '--no-graph[Manually specify whether a dependency graph should be created and/or skipped.
        This command will automatically detects whether a graph is needed, based on whether dependencies should be considered when inflating yamls.
        This flag is useful if you'\''d like to inflate a module'\''s yaml without considering its dependencies.]'
        ':input-file:'
        '*'{--verbosity,-v}'[Control the verbosity of the CLI, the more times you pass the flag, the more verbose the output. Use `ios <command> -vvv` for the most verbose output.]'
        '--log-to-file[In addition to printing to the console, log to root/ios-cli.log]'
        '(--skip --skip-prompts)'{--skip,--skip-prompts}'[This option will skip all prompts in the ios-cli tool]'
        '--git-lfs-verification[Should the CLI run git LFS verifications]'
        '--no-git-lfs-verification[Should the CLI run git LFS verifications]'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
}


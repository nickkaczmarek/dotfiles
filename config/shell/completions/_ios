        '(--repo -r)'{--repo,-r}'[The repo to use for artifactory. Defaults to "libs-release-local" if no repo supplied.]:repo:'
        '(--json-config -j)'{--json-config,-j}'[An input file containing a JSON config to use.
        It may contain any of the options\: "username", "password", "url", and "repo" (corresponding to the similarly-named options for this command).
        If these options are passed directly to a command, they override the options found in the config.]:json-config:'
        '(-h --help)'{-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S : "${arg_specs[@]}" && ret=0

    return "${ret}"
